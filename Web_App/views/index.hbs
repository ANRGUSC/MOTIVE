<form action="/beaconingA" method="post">
<div class="panel panel-warning">
  <div class="panel-heading">
    <h3 class="panel-title">Expected Time To Be in Range</h3>
  </div>
  <div class="panel-body">
    <div class="form-group">
    <input type="text" id="time_contact" name="time_contact" class="form-control" placeholder="Enter Time in (s)" aria-describedby="basic-addon1">
  </div>
</div>
</div>

<div class="form-group">
<div class="panel panel-warning">
  <div class="panel-heading">
    <h3 class="panel-title">Duration of Being in Range</h3>
  </div>
  <div class="panel-body">
    <input type="text" id="duration" name="duration" class="form-control" placeholder="Enter Time in (s)" aria-describedby="basic-addon1">
  </div>
</div>
</div>

<button type="submit" class="btn btn-success btn-block">Start!</button>
</form>
